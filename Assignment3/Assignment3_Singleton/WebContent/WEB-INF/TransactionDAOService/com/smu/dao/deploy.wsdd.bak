<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TransactionDAOService WSDL service -->

  <service name="TransactionDAO" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://dao.smu.com"/>
      <parameter name="wsdlServiceElement" value="TransactionDAOService"/>
      <parameter name="schemaQualified" value="http://dao.smu.com,http://dto.smu.com"/>
      <parameter name="wsdlServicePort" value="TransactionDAO"/>
      <parameter name="className" value="com.smu.dao.TransactionDAOSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TransactionDAO"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="readDataBase" qname="operNS:readDataBase" xmlns:operNS="http://dao.smu.com" returnQName="retNS:readDataBaseReturn" xmlns:retNS="http://dao.smu.com" returnType="rtns:TransactionDTO" xmlns:rtns="http://dto.smu.com" soapAction="" >
        <parameter qname="pns:trnxId" xmlns:pns="http://dao.smu.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="deleteRecord" qname="operNS:deleteRecord" xmlns:operNS="http://dao.smu.com" returnQName="retNS:deleteRecordReturn" xmlns:retNS="http://dao.smu.com" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:txnID" xmlns:pns="http://dao.smu.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="updateRecord" qname="operNS:updateRecord" xmlns:operNS="http://dao.smu.com" returnQName="retNS:updateRecordReturn" xmlns:retNS="http://dao.smu.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:ID" xmlns:pns="http://dao.smu.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:nameOnCard" xmlns:pns="http://dao.smu.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="createResultSetInTable" qname="operNS:createResultSetInTable" xmlns:operNS="http://dao.smu.com" returnQName="retNS:createResultSetInTableReturn" xmlns:retNS="http://dao.smu.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:ID" xmlns:pns="http://dao.smu.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:NameOnCard" xmlns:pns="http://dao.smu.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:CardNumber" xmlns:pns="http://dao.smu.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:UnitPrice" xmlns:pns="http://dao.smu.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:Quantity" xmlns:pns="http://dao.smu.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:ExpDate" xmlns:pns="http://dao.smu.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:CreditCardType" xmlns:pns="http://dao.smu.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="readDataBase updateRecord createResultSetInTable deleteRecord"/>

      <typeMapping
        xmlns:ns="http://dto.smu.com"
        qname="ns:TransactionDTO"
        type="java:com.smu.dto.TransactionDTO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
